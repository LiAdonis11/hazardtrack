{
  "name": "hazardtrack-mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "clean": "if exist node_modules\\.cache rmdir /s /q node_modules\\.cache && if exist .expo rmdir /s /q .expo && if exist dist rmdir /s /q dist",
    "clean:android": "if exist android\\app\\.cxx rmdir /s /q android\\app\\.cxx && if exist android\\.gradle rmdir /s /q android\\.gradle && if exist android\\build rmdir /s /q android\\build && if exist android\\app\\build rmdir /s /q android\\app\\build",
    "reset": "npm run clean && if exist node_modules rmdir /s /q node_modules && npm install",
    "fix-web": "npm run clean && npx expo install --fix -- --legacy-peer-deps && EXPO_USE_REANIMATED_PLUGIN=false npx expo start --web --clear",
    "optimize": "npx expo export --platform android",
    "tamagui-generate": "npx tamagui generate",
    "build:prod": "NODE_ENV=production EXPO_PUBLIC_TAMAGUI_TARGET=native eas build --platform android --profile production",
    "build:preview": "eas build --platform android --profile preview",
    "prebuild": "npm run clean && npm run type-check",
    "type-check": "tsc --noEmit"
  },
  "jest": {
    "preset": "jest-expo"
  },
