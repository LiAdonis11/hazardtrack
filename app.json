{
  "expo": {
    "name": "hazardtrack",
    "slug": "hazardtrack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logos/hazartrack-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logos/hazartrack-logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.liadonis11.hazardtrack",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": false,
      "predictiveBackGestureEnabled": false,
      "package": "com.liadonis11.hazardtrack",
      "permissions": [
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.INTERNET",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/logos/hazartrack-logo.png"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/logos/hazartrack-logo.png",
          "color": "#D62828",
          "defaultChannel": "default"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          },
          "ios": {}
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "0f3ab0f9-7806-4d0f-83f6-fa961c1d962b"
      },
      "router": {}
    },
    "owner": "hera200318",
    "scheme": "hazardtrack-mobile"
  }
}
